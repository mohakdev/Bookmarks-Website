@page
@using Bookmarks.Models
@model IndexModel
@{
    ViewData["Title"] = "Bookmarks";
}

<div id = "allBookmarks">
	@foreach(Bookmark bookmark in Model.Bookmark)
	{
		<div class = "Bookmark">
			<img src= "https://logo.clearbit.com/@bookmark.Url?size=128.jpg" height="128" width="128"/>
			<a href="https://@bookmark.Url">@bookmark.Name</a>
			
		</div>
	}
</div>